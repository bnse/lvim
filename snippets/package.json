        "path": "./dot.json"
      },
      {
        "language": "go",
        "path": "./go.json"
      },
      {
        "language": "markdown",
        "path": "./markdown.json"
      },
      {
        "language": "zig",
        "path": "./zig.json"
      }
    ]
  }
}
